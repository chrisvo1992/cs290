<h1>Workout Tracker</h1>
<div>
    <fieldset>
        <label>Enter new entry: </label>
        <form>
            <label>Name</label>
            <input type="text" id="name"> <br>
            <label>reps</label>
            <input type="text" id="reps"> <br>
            <label>weight</label>
            <input type="text" id="weight"> <br>
            <label>unit (1 for lbs, 0 for kgs)</label>
            <input type="text" id="unit">
            <br>
            <input type="submit">
        </form>
    </fieldset>
</div>
<br>
<table border="1px solid black">
  <tr>
    <th> Name </th>
    <th> Reps </th>
    <th> Weight </th>
    <th> Date </th>
    <th> Unit (1= lbs, 0 = Kgs) </th>
  </tr>
  {{#each results}}
  <tr>
    <td>{{this.name}}</td> 
    <td>{{this.reps}}</td>
    <td>{{this.weight}}</td>
    <td>{{this.date}}</td>
    <td>{{this.lbs}}</td>
    <td><button type="button" id='delete{{this.id}}'>Delete Row</button> 
    <button type="button" id='edit{{this.id}}'>Edit Row</button></td>
  </tr>
  {{/each}}

</table>



